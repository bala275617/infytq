#PF-Prac-26
def check_occurence(string):
    #start writing your code here
    string=string.lower()
    a=string.count("jet")
    b=string.count("mat")
    if(a==b):
        return True
    else:
        return False
        
string="Jet on the Mat but mat is too long"
print(check_occurence(string))
